<h1 class="posttitle dylanrichards pagetitle">
  <i class="fa fa-pencil fa-2x"></i>
</h1>

<div id="post">

  <% if current_user %>
    <%= link_to 'New Post', new_post_path %>
  <% end %>

  <br>

  <ul class="index-posts-list">
    <%= render @posts %>
  </ul>

  <%= will_paginate @posts %>
</div>

